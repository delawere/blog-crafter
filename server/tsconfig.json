  "compilerOptions": {
    "declaration": true,
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
